    <ProjectReference Include="..\..\vnc\VNC.Core\VNC.CoreNF.csproj" />
    <ProjectReference Include="..\..\vnc\VNC.Logging\VNC.Logging\VNC.LoggingNF.csproj" />
  </ItemGroup>-->
